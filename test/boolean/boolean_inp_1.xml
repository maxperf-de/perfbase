<?xml version="1.0" standalone="no"?>
<!DOCTYPE input SYSTEM "../../dtd/pb_input.dtd">

<input>
  <experiment>boolean_TEST</experiment>

  <named_location store_set="yes">
    <name>l</name>
    <match>latency:</match>
  </named_location>

  <named_location content="leads">
    <name>N_proc</name>
    <match>processes</match>
  </named_location>
  
  <named_location>
    <name>IXS</name>
    <match>IXS:</match>
    <map>
      <content_in>on</content_in>
      <content_out>t</content_out>
    </map>
    <map>
      <content_in>off</content_in>
      <content_out>f</content_out>
    </map>
  </named_location>

</input>


