  </parameter>

  <parameter>
    <name>t</name>
    <synopsis>time</synopsis>

    <datatype>float</datatype>
    <unit>
      <base_unit>s</base_unit>
    </unit>
  </parameter>

  <result>
    <name>v</name>
    <synopsis>value</synopsis>

    <datatype>integer</datatype>
    <unit>
      <base_unit>none</base_unit>
    </unit>
  </result>

</experiment>


