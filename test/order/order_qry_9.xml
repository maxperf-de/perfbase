<?xml version="1.0" standalone="no"?> 
<!DOCTYPE query SYSTEM "../../dtd/pb_query.dtd">

<query id="showall">
  <experiment>order_TEST</experiment>

  <parameter id="p.p" show="data">
    <value>p</value>
  </parameter>

  <parameter id="p.T0" show="filter">
    <value>T</value>
    <filter>
      <equal>0</equal>
    </filter>
  </parameter>

  <parameter id="p.T1" show="filter">
    <value>T</value>
    <filter>
      <equal>1</equal>
    </filter>
  </parameter>

  <source id="src.r0">
    <result>r</result>      

    <input>p.p</input>
    <input>p.T0</input>
  </source>

  <source id="src.r1">
    <result>r</result>      

    <input>p.p</input>
    <input>p.T1</input>
  </source>

  <operator id="op.r" type="sort" value="r" variant="descending">
    <input>src.r0</input>
    <input>src.r1</input>
  </operator>

  <output id="out" target="raw_text">
    <input>op.r</input>
  </output>
</query>
