<?xml version="1.0" standalone="no"?>
<!DOCTYPE experiment SYSTEM "../../dtd/pb_experiment.dtd">

<experiment>
  <name>regexp_TEST</name>

  <info>
    <performed_by> 
      <name>Joachim Worringen</name>
      <organization>Computer and Communications Research Lab, NEC Europe Ltd.</organization>
    </performed_by> 
    <project>perfbase testsuite</project>
    <synopsis>Test for parsing with regular expressions</synopsis>
    <description>
      Test different types of locations with regular expressions
    </description>
  </info>

  <parameter occurrence="once">
    <name>N_proc</name>
    <synopsis>number of processes</synopsis>

    <datatype>integer</datatype>    
    <unit>
      <base_unit>process</base_unit>
    </unit>
  </parameter>
 
  <result>
    <name>l</name>
    <synopsis>latency</synopsis>

    <datatype>float</datatype>
    <unit>
      <base_unit>s</base_unit>
      <scaling>micro</scaling>
    </unit>
  </result>

</experiment>


