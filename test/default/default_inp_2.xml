<?xml version="1.0" standalone="no"?>
<!DOCTYPE input SYSTEM "../../dtd/pb_input.dtd">

<input>
  <experiment>default_TEST</experiment>
    
  <named_location>
    <name>type</name>
    <match>vehicle_type:</match>
  </named_location>

  <named_location>
    <name>v</name>
    <match>speed:</match>
  </named_location>

  <tabular_location>
    <match># emission</match>
    <skip>1</skip>

    <tabular_value>
      <name>rpm</name>
      <pos>0</pos>
    </tabular_value>

    <tabular_value>
      <name>CO_2</name>
      <pos>1</pos>
    </tabular_value>
  </tabular_location>

</input>
