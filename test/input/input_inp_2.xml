<?xml version="1.0" standalone="no"?>
<!DOCTYPE input SYSTEM "../../dtd/pb_input.dtd">

<input>
  <experiment>input_TEST</experiment>

  <tabular_location columns="2">
    <match># msg_size</match>
    <skip>1</skip>

    <tabular_value>
      <name>S_msg</name>
      <pos>0</pos>
    </tabular_value>

    <tabular_value>
      <name>T_pp</name>
      <pos>1</pos>
    </tabular_value>
  </tabular_location>

  <named_location is_separator="yes">
    <name>IC</name>
    <match>interconnect:</match>
  </named_location>

  <named_location>
    <name>bufmode</name>
    <match>buffering:</match>
  </named_location>

</input>
