<?xml version="1.0" standalone="no"?> 
<!DOCTYPE query SYSTEM "../../dtd/pb_query.dtd">

<query id="onesrc">
  <experiment>oponesrc_TEST</experiment>

  <parameter id="p.p">
    <value>p</value>
  </parameter>

  <source id="src.r">
    <result>r</result>      
    <input>p.p</input>
  </source>

  <operator id="op" type="median">
    <input>src.r</input>
  </operator>

  <operator id="sort" type="sort">
    <input>op</input>
  </operator>

  <output id="out" target="raw_text">
    <input>sort</input>
    <filename>qry_result</filename>
  </output>
</query>
